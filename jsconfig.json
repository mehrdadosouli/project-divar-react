{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": ".",
        "paths": {
            "src/*":["/src/*"],
            "services":["/src/services"]
        }
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}